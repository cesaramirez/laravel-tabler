{
  "name": "cesaramirez/laravel-tabler",
  "description": "Laravel 5.5/5.6 Front-end preset for Tabler Bootstrap.",
  "keywords": ["laravel", "preset", "tabler", "bootstrap"],
  "license": "MIT",
  "minimum-stability": "stable",
  "authors": [
    {
      "name": "César Ramírez",
      "email": "cesar.ramirez.sv@gmail.com"
    }
  ],
  "require": {
    "laravel/framework": "^5.5"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.5"
  },
  "autoload": {
    "psr-4": {
      "cesaramirez\\Presets\\Tabler\\": "src/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    },
    "laravel": {
      "providers": ["cesaramirez\\Presets\\Tabler\\TablerPresetServiceProvider"]
    }
  }
}
